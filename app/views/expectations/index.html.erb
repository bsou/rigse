<h1>Listing expectations</h1>

<table>
  <tr>
  </tr>

<% for expectation in @expectations %>
  <tr>
    <td><%= link_to 'Show', expectation %></td>
    <td><%= link_to 'Edit', edit_expectation_path(expectation) %></td>
    <td><%= link_to 'Destroy', expectation, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New expectation', new_expectation_path %>
