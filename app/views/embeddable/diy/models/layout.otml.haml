!!! XML
= render :partial => "otml/character_entities"
%otrunk{ :id => @model.uuid}
  =ot_imports
  %objects
    %OTSystem
      =ot_bundles(:title => h(@model.name))
      %root
        %OTCompoundDoc
          %bodyText
            #content
              %h3
                = h(Embeddable::NLogoModel.display_name + ': ' + @model.name)
              %object{ :refid => ot_refid_for(@model) }
      %library
        =yield :layout
