%h1= "Data Collectors: #{@data_collectors.length}" 
- content_for(:content_header_left) do
  %ul
    - if RAILS_ENV == "development"
      %li
-content_for(:content_header_right) do
  %ul
    %li

- @data_collectors.each do |data_collector|
  %div{ :id => "item_#{data_collector.name.underscore}_#{data_collector.id}", :class => 'item' }
    = render :partial => 'data_collector', :locals => { :data_collector => data_collector }
