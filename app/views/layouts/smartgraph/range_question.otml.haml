!!! XML
= render :partial => "otml/character_entities"
%otrunk{ :id => @range_question.uuid}
  =ot_imports
  %objects
    %OTSystem
      =ot_bundles(:title => @range_question.name)
      %root
        %OTCompoundDoc
          %bodyText
            #content
              %h3
                = Smartgraph::RangeQuestion.display_name + ': ' + @range_question.name
              %object{ :refid => ot_refid_for(@range_question) }
      %library
        =yield :layout