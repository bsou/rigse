.action_menu
  .action_menu_header
    .action_menu_header_left
      .left_content
        - form_tag url_for(:action => "index", :page => session[:page] || 1 ), :method => 'get' do
          Grade Span: 
          - content_tag :label do
            = text_field_tag :search, params[:search], :size => 16
          %input{ :type => "submit", :value => "Search"}
        - if @paginated_objects
          = page_entries_info @paginated_objects
        = will_paginate @paginated_objects
    .action_menu_header_right
      %ul.menu
        %li= toggle_all 'gse descriptions'

  
  %p.paginator
    

= render :partial => 'expectation_list', :locals => { :grade_span_expectations => @grade_span_expectations }
