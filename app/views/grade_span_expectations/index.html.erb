<h1>Listing grade_span_expectations</h1>

<table>
  <tr>
  </tr>

<% for grade_span_expectation in @grade_span_expectations %>
  <tr>
    <td><%=h grade_span_expectation.grade_span %></td>
    <td><%= link_to 'Show', grade_span_expectation %></td>
    <td><%= link_to 'Edit', edit_grade_span_expectation_path(grade_span_expectation) %></td>
    <!-- <td><%= link_to 'Destroy', grade_span_expectation, :confirm => 'Are you sure?', :method => :delete %></td> -->
  </tr>
  <% assessment_target = grade_span_expectation.assessment_target %>
  <% if assessment_target %>
    <tr>
      <td>Assessment Target</td>
      <td><%=h assessment_target.number %></td>
      <td><%=h assessment_target.grade_span %></td>
    </tr>
    <tr>
      <td><%= assessment_target.description %></td>
    </tr>
  <% end %>
<% end %>
</table>

<br />

<%= link_to 'New grade_span_expectation', new_grade_span_expectation_path %>
